/* pages/more/more.wxss */
.practical{
  width: 100%;
  height: 62rpx;
  background: #e8e8e8;
  line-height: 62rpx;
  color: #a3a3a3;
  font-size: 30rpx;
  text-indent: 30rpx;
}
.practical-nav{
  width: 100%;
  height: auto;
  padding-bottom: 30rpx;
}
.practical-nav::after{
  content: '';
  clear: both;
  display: block;
}
.nav{
  width: 170rpx;
  height: 170rpx;
  background: #e8e8e8;
  margin-left: 14rpx;
  margin-top: 14rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  float: left;
}
.nav-img{
  width: 100rpx;
  height: 100rpx;
}
.nav-txt{
  font-size: 24rpx;
  margin-top: 8rpx;
}